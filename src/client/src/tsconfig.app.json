{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "paths": {
      "@app/*": [
        "./app/*"
      ],
      "@shared/*": [
        "../../shared/*"
      ]
    },
    "target": "es5",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
